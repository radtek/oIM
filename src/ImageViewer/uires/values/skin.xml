<?xml version="1.0" encoding="utf-8"?>

<skin>
	<!--背景图片 -->
	<imgframe name="skin.sys.bkgnd" src="IMG:SYS_BKGND" margin="5,3,5,7" />
	<imgframe name="skin.toolbar.bkgnd" src="IMG:TOOLBAR_BKGND" margin="4,4,4,4" />
	<imgframe name="skin.thumb.bkgnd" src="IMG:THUMB_BKGND" margin="4,4,4,4" />

	<!-- 系统图片 -->
	<imglist name="skin.traymenu.icons" src="IMG:traymenu_icons" states="9"/>
	<imglist name="skin.sys.close" src="IMG:SYS_CLOSE" states="3" />
	<imglist name="skin.error" src="IMG:IMG_ERROR" />
	<imglist name="skin.ratio" src="IMG:IMG_RATIO" />

	<!-- 地图 -->
	<imgframe name="skin.thumb.pos" src="IMG:THUMB_POS" margin="2,2,2,2" />
	<imglist name="skin.thumb.toggle" src="IMG:THUMB_TOGGLE" states="6"/>

	<!-- 按钮 -->
	<imglist name="skin.btn.prev" src="IMG:BTN_PREV" states="4" />
	<imglist name="skin.btn.next" src="IMG:BTN_NEXT" states="4" />
	<imglist name="skin.btn.left" src="IMG:BTN_LEFT" states="3" />
	<imglist name="skin.btn.right" src="IMG:BTN_RIGHT" states="3" />

	<!-- 工具栏 -->
	<imglist name="skin.toolbar.1to1" src="IMG:TOOLBAR_1TO1" states="4" />
	<imglist name="skin.toolbar.fullscreen" src="IMG:TOOLBAR_FULLSCREEN" states="6"/>
	<imglist name="skin.toolbar.zoomout" src="IMG:TOOLBAR_ZOOMOUT" states="4"/>
	<imglist name="skin.toolbar.zoomin" src="IMG:TOOLBAR_ZOOMIN" states="4"/>
	<imglist name="skin.toolbar.rotate" src="IMG:TOOLBAR_ROTATE" states="4"/>
	<imglist name="skin.toolbar.folder" src="IMG:TOOLBAR_FOLDER" states="4"/>
	<imglist name="skin.toolbar.saveas" src="IMG:TOOLBAR_SAVEAS" states="4"/>
	<imglist name="skin.toolbar.toggle" src="IMG:TOOLBAR_TOGGLE" states="6"/>

</skin>
