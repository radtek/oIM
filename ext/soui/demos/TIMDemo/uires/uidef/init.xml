<?xml version="1.0" encoding="utf-8"?>
<UIDEF>
    <font face="微软雅黑" size="12" />

    <!--2.0重要更新：string,color,skin这3个节点在有src属性时切换到src指定的XML中去加载，style, objattr保持不变-->
  
    <!--定义字符串表-->
    <string src="values:string"/>
    <!--定义颜色值表-->
    <color src="values:color"/>
    <!--定义全局skin对象-->
    <skin src="values:skin"/>

    <style>       
        <class name="cls_title" font="face:微软雅黑,size:16" />
        <class name="cls_link" cursor="hand" colorText="#1e78d5" font="size:13" colorTextHover="#1e78d5" fontHover="size:13,underline:1"/>
        <class name="combo_box" inset="10,0,0,0" drawFocusRect="0" dropDown="1" ncSkin="broder_nor" margin="1" dotted="0" animateTime="200"/>
		<class name="combo_list" ncSkin="broder_nor" margin="1" iconSkin="" itemSkin="" itemHeight="28" text-x="10" colorBkgnd="#FFFFFF" colorItemBkgnd="#FFFFFF" colorSelText="#FFFFFF" colorText="#000000"/>

    </style>

    <objattr>
        <edit inset="4,0,2,0" margin="1" cueColor="@color/disable"/>
	</objattr>
</UIDEF>